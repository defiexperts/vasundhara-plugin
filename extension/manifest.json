{
  "manifest_version": 2,
  "name": "CSV Form Filler",
  "version": "1.0",
  "permissions": [
    "storage",
    "activeTab",
    "tabs",
    "http://*/*",
    "https://*/*",
    "executeScript"
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  },
  "content_scripts": [
    {
      "matches": ["https://majhivasundhara.in/en/pledge/*"],
      "js": ["jquery-1.11.0.min.js", "script.js"]
    }
  ]
}
